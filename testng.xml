<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite_Demo">
	<parameter name="urlQAlified" value="https://qalified.com/" />
	
	<parameter name="browser" value="CHROME" />
	<parameter name="max_wait" value="50" />
	<parameter name="isHeadless" value="false" />
	<parameter name="close_browser_after_execution" value="true" />
    <parameter name="newBase" value="false" />

	<test thread-count="5" name="testSuite-test">
		<classes>
			<class name="test.TestCaptureComplete" />
			<class name="test.TestCaptureElement" />
			<class name="test.TestCaptureHiddenElement" />
		</classes>
	</test>
</suite> <!-- Suite -->
	

